{
  "name": "@iterate-com/estate-garple",
  "type": "module",
  "dependencies": {
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "5.85.5",
    "@trpc/client": "^11.5.0",
    "@trpc/server": "^11.5.0",
    "@trpc/tanstack-react-query": "^11.5.0",
    "@uidotdev/usehooks": "^2.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "hono": "^4.9.4",
    "lucide-react": "0.542.0",
    "posthog-js": "^1.261.0",
    "typeid-js": "^1.2.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.8.2",
    "resend": "^4.7.0",
    "stripe": "^18.3.0",
    "superjson": "^2.2.2",
    "tailwind-merge": "^2.6.0",
    "zod": "^4.0.17"
  },
  "scripts": {
    "dev": "cd apps/website && CLOUDFLARE_INCLUDE_PROCESS_ENV=true doppler run -- vite dev",
    "build": "cd apps/website && doppler run -- vite build",
    "deploy": "pnpm run build && cd apps/website && doppler run -- pnpx wrangler deploy",
    "typecheck": "cd apps/website && tsc --noEmit",
    "test": "cd apps/website && vitest run --passWithNoTests",
    "lint": "cd apps/website && eslint . --fix",
    "lint:check": "cd apps/website && eslint . --max-warnings 0",
    "db:generate": "cd apps/website && drizzle-kit generate",
    "db:migrate": "cd apps/website && doppler run -- pnpx wrangler d1 migrations apply estate-garple-production-garple-website-d1 --local",
    "db:migrate:production": "cd apps/website && doppler run -- pnpx wrangler d1 migrations apply estate-garple-production-garple-website-d1 --remote",
    "db:studio:garple": "cd apps/website && drizzle-kit studio"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.2",
    "@cloudflare/workers-types": "^4.20250913.0",
    "@iterate-com/sdk": "workspace:*",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "^0.31.4",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^7.1.3",
    "vitest": "^3.1.7",
    "wrangler": "^4.33.0"
  }
}
