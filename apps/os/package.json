  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types --no-include-runtime",
    "deploy": "doppler run -- pnpm run build && doppler run -- wrangler deploy",
    "dev": "pnpm iterate dev start",
    "eval": "doppler run -- evalite run-once && tsx evals/evalite-exporter.ts",
    "eval:serve": "pnpx serve ignoreme/evalite-ui",
    "eval:watch": "doppler run -- evalite watch",
    "iterate": "doppler run -- tsx --import ./sdk/cli/cf-shim/loader.ts ./sdk/cli/index.ts",
    "preview": "pnpm run build && vite preview",
    "test": "doppler run -- vitest",
    "test:watch": "doppler run -- vitest --watch",
    "typecheck": "pnpm run cf-typegen && react-router typegen && tsc -b",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio"
  },
